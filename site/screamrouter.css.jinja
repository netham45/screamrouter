SPAN.route-label {
    font-size: 1vw;
    pointer-events: none;
    display: inline-block;
    padding-bottom: 15px;
    color: #333;
    font-weight: bold;
    font-style: italic;
}

DIV#enable_disable {
    z-index: -100;
    position:  relative;
    min-height: 100%;

}

@media only screen and (max-width: 825px) {
    DIV.section {
        width: 100%;
        margin-top: 5px;
    }

    BUTTON {
        padding-top: 5px;
        padding-bottom: 5px;
    }

    DIV.select-container {
        font-size: inherit;
        position: relative;
    }

    DIV.main-select {
        font-size: inherit;
        position: relative;
    }

    SPAN.route-label {
        font-size: 8pt;
        color: #111111;
        pointer-events: none;
    }


    DIV#dialog {
        position: absolute;
        top: 0%;
        left: 0px;
        padding-top: 0px;
        padding-left: 0px;
        padding-right: 0px;
        margin-left: 0px;
        max-width: 100%;
    }
    DIV.title {
        font-family: "Courier";
        font-weight: 1000;
        font-size: 7.8vw;
    }
    span.button-icon {
        font-size: 8vw;
    }

  }

@media (pointer: coarse)  {
    #update_source_button, #update_sink_button, #update_route_button, 
    #update_source_equalizer_button, #update_sink_equalizer_button, #update_route_equalizer_button,
    #enable_disable_source_button, #enable_disable_route_button, #enable_disable_sink_button,
    #listen_to_sink_button
    {
        display: inline;
    }
    DIV#vnc-on-clck {
        display: block;
        z-index: 2000;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }
    SPAN.option-drag {
        font-size: 18pt;
    }
}

DIV.option-buttons {
    display: none;
}

SPAN.option-selected DIV.option-buttons {
    display: block !important;
}

SPAN.option-drag {
    float: right;
}