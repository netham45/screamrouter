DIV.blur {
    backdrop-filter: blur(15px);
    height: 100%;
    width: 100%;
    border-collapse: collapse;
    overflow: hidden;
    padding-top: 10%;
    margin-top: -10%;
    padding-bottom: 10%;
    margin-bottom: -10%;
}

DIV#dialog LABEL {
    float: left;
}

DIV#dialog INPUT[TYPE="text"], DIV#dialog SELECT {
    float: right;
    display: block;
}

DIV#dialog INPUT[TYPE="text"] {
    width: 50%;
}

DIV#dialog SELECT {
    width: 52%;
}

DIV#dialog BUTTON {
    vertical-align: middle;
    font-size: 14pt;
    background-color: #6CB050;
    color: #FFFFFF;
    font-family: "Open Sans", sans-serif;
    border-radius: 8px;
    border: none;
    padding: 4px;
    padding-left: 8px;
    padding-right: 8px;
    margin: 2px;
    display: inline;
}

DIV#dialog SPAN {
    display: inline-block;
    width: 100%;
}

DIV#dialog SPAN.equalizer {
    width: 50px;
}

AUDIO {
    position: fixed;
    margin-left: 50px;
    margin-top: 15px;
}

OPTION {
    color: #000000;
    background-color: ;
}

DIV#dialog INPUT.eq_slider {
    padding-top: 5px;
    appearance: slider-vertical;
    width: 20px;
}

DIV#shadow {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    backdrop-filter: blur(2px);
    display: none;
}

span.button-icon, span.icon {
    font-size: 1.5vw;
    text-shadow: 0 0 2px #777, 0 0 12px #444;
}

#update_source_button, #update_sink_button, #update_route_button, 
#update_source_equalizer_button, #update_sink_equalizer_button, #update_route_equalizer_button,
#enable_disable_source_button, #enable_disable_route_button, #enable_disable_sink_button,
#listen_to_sink_button
{
    display: none;
}

BUTTON#disable_route {
    display: none;
}

SELECT.main-select option {
    height: 14pt;
    overflow: hidden;
    padding-top: -15px;
    padding-bottom: 10px;
}

SELECT.main-select option::before {
    white-space: nowrap;
    font-size: 14pt;
}

SPAN.option-editor-Enable {

}

SELECT.main-select OPTION[DATA-TYPE="SinkDescription"].option-enabled::before {
    content: "🎚️⚙️🔊📻";
}

SELECT.main-select OPTION[DATA-TYPE="SinkDescription"].option-disabled::before {
    content: "🎚️⚙️🔇📻";
}

SPAN.option-label {
    overflow: visible;
}

DIV#presetControls {
    padding-top: 15px;
    color: #000000;
    font-size: 14pt;
    text-shadow: 0 0 20px #fff, 0 0 30px #AA7777, 0 0 40px #AA7777, 0 0 50px #AA7777, 0 0 60px #AA7777, 0 0 70px #AA7777, 0 0 80px #AA7777;
    text-align: left;
}

DIV#mainWrapper {
    display: none;
    left: 0px;
    top: 0px;
    width: 100%;
}

canvas#canvas {  
    bottom: 0px;
    margin-bottom: -5px;
    width: 720px;
    height: 480px;
}

IFRAME#vnc-iframe {
    z-index: 1000;
}

DIV#vnc-on-clck {
    display:none;
}

SPAN.route-label {
    font-size: 1vw;
    pointer-events: none;
    display: inline-block;
    padding-bottom: 15px;
    color: #ded;
    font-weight: bold;
    font-style: italic;
}

DIV#enable_disable {
    z-index: -100;
    position:  relative;
    min-height: 100%;

}

SPAN.mobile-route-buttons {
    display: none;
}

@media only screen and (max-width: 825px) {
    DIV.section {
        width: 100%;
        margin-top: 5px;
    }

    BUTTON {
        padding-top: 5px;
        padding-bottom: 5px;
    }

    DIV.select-container {
        font-size: 24pt;
        position: relative;
    }

    DIV.main-select {
        font-size: 24pt;
        position: relative;
    }

    SPAN.route-label {
        font-size: 8pt;
        color: #111111;
        pointer-events: none;
    }


    DIV#dialog {
        position: absolute;
        top: 0%;
        left: 0px;
        padding-top: 0px;
        padding-left: 0px;
        padding-right: 0px;
        margin-left: 0px;
        max-width: 100%;
    }
    DIV.title {
        font-family: "Trebuchet MS";
        font-weight: 1000;
        font-size: 7.8vw;
    }
    span.button-icon {
        font-size: 8vw;
    }
    SPAN.mobile-route-buttons {
        display: inline;
    }
    SPAN.desktop-route-buttons {
        display: none;
    }
    DIV#select-routes { 
       display: block !important;
    }

    SPAN#route-buttons {
        display: none;
    }

    svg#routeLines { 
        display: none;
    }
    SPAN.option{
        font-size: inherit;
    }
  }

@media (pointer: coarse)  {
    #update_source_button, #update_sink_button, #update_route_button, 
    #update_source_equalizer_button, #update_sink_equalizer_button, #update_route_equalizer_button,
    #enable_disable_source_button, #enable_disable_route_button, #enable_disable_sink_button,
    #listen_to_sink_button
    {
        display: inline;
    }
    DIV#vnc-on-clck {
        display: block;
        z-index: 2000;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }
    SPAN.option-drag {
        font-size: 18pt;
        float: right;
    }
}

DIV.option-buttons {
    display: none;
}

SPAN.option-selected DIV.option-buttons {
    display: block !important;
}

SPAN.option-drag {
    float: right;
    vertical-align: middle;
    color: #000;
    text-shadow: none;
}

SPAN.option-selected > DIV {
    color: #000;
}

SPAN.option DIV, SPAN.option SPAN {
    font-style: normal;
}

SPAN.option[data-is_group="True"]  {
    font-style: italic;
}

SPAN.icon-enabledisable {
    height: 100%;
    vertical-align: middle;
    display: block;
    pointer-events: auto;
    float: right;
}

.glowline {
    pointer-events: auto;
}

.glowline:hover {
    stroke: #FF0;
}

button.button-option-disable, SPAN.option-editor-Disable BUTTON.enable-button {
    background-color: #666 !important;
}

SPAN.option-editor-Disable BUTTON.disable-button {
    display: none;
}

SPAN.option-editor-Enable BUTTON.enable-button {
    display: none;
}