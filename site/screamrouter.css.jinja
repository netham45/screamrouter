
DIV#select-routes { 
    display: none;
}

DIV#dialog {
    padding-top: 20px;
    padding-bottom: 20px;
    position: absolute;
    min-width: 400px;
    max-width: 95%;
    height: auto;
    top: 12.5%;
    margin-left: -16.5%;
    background: none;
    background-image: linear-gradient( rgba(40,120,60,.7), rgba(20,80,40,.5));
    border-radius: 8px;
    display: none;
    backdrop-filter: blur(5px);
    font-family: "Trebuchet MS";
    font-weight: 1000;
    text-shadow: 0 0 20px #fff, 0 0 30px #000, 0 0 40px #000;
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    left: 0;
    right: 0;
    color: #FFF;
}

DIV.title {
    font-family: Orbitron, sans-serif;
    font-weight: bold;
    font-size: 48px;
    text-shadow: 0 0 20px #224411, 0 0 30px #224411, 0 0 40px #224411, 0 0 50px #224411;
    color: #7FFF00;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
    width: 100%;
    overflow: hidden;
    border-collapse: collapse;
    background-color: rgba(00,120,00,.2);
    border-radius: 5px;
    background-size: 60vmin 60vmin;
    display: inline-block;
    border-collapse: collapse;
}

DIV.section {
    pointer-events: auto;   
    overflow: hidden;
    border-collapse: collapse;
    background-color: rgba(00,120,00,.2);
    border-radius: 5px;
    background-size: 60vmin 60vmin;
    width: 33%;
    border-collapse: collapse;
    top: 0px;
    position: relative;
    display: inline-block;
}

DIV.section-name {
    font-family: Lato, sans-serif;
    font-weight: bold;
    font-size: 28px;
    color: #32CD32;

    text-shadow: 0 0 10px #224411, 0 0 30px #224411;
    text-align: center;
    background-image: linear-gradient( rgba(80,160,120,.6), rgba(20,70,30,.1));
}

DIV.section INPUT[TYPE="range"] {
    width: 96%;
    display: inline-block;
    margin-left: 2%;
}

DIV.section-buttons BUTTON {
    vertical-align: middle;
    font-size: 14pt;
    background-color: #6CB050;
    color: #FFFFFF;
    font-family: "Open Sans", sans-serif;
    border-radius: 8px;
    border: none;
    padding: 4px;
    padding-left: 8px;
    padding-right: 8px;
    margin: 2px;
}

DIV.section-buttons BUTTON[disabled] {
    background-color: #2c3F10;
    color: #BBBBBB;
}

DIV.section-buttons {
    text-align: center;
}


DIV#reload {
    height: 33%;
    pointer-events: none;
}

DIV.main-select {
    margin-top: 5px;
    padding-top: 8px;
    font-family: sans-serif;
    overflow: auto;
    text-align: center;
    width: 100%;
    background: none;
    background-image: linear-gradient( rgba(120,240,160,.4), rgba(100,220,140,.2));
    font-size: 1.5vw;
    color: #FFFFFF;
    border: none;
    outline: none;
    padding-bottom: 15px;
    border-radius: 5px;
}

DIV.main-select > SPAN.option:nth-child(even) {
    background-image: linear-gradient(rgba(0,0,0,.2), rgba(0,0,0,.2));
}

DIV.main-select > SPAN.option:nth-child(odd) {
    background-image: linear-gradient(rgba(0,0,0,.1), rgba(0,0,0,.1));
}

SPAN.option{
    font-family: "Lato", sans-serif;
    font-size: 1.75vw;
    padding-left: 2%;
    width: 95%;
    display: inline-block;
    text-align: left;

    border: 0px 0px 2px 0px;
    padding-top: 8px;
    padding-bottom: 12px;
    border-radius: 10px;
    color: #ffd;
    margin-top: .5px;
    margin-bottom: .5px;
}

select[multiple] option:checked {
  background-image: linear-gradient(rgba(140,190,140,.75),rgba(0,140,0,.75),rgba(140,190,140,.75));
}

select[multiple]:focus option:checked {
  background-image: linear-gradient(rgba(220,255,220,.75),rgba(0,192,0,.75),rgba(255,255,255,.75));
}

option:active, option:focus, option:hover, option:checked:after {
    outline: none;
}

CANVAS#backgroundCanvas {
    display: block;
    filter: blur(14px);
    position: absolute;
    top: 0px;
    left: 0px;
    z-index: -999;
}

HTML, BODY {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background-color: #000;
    background-attachment: fixed;
    background-position: center;
    background-size: 50% 50%;
    height: 100%;
    background-color: #020202;
    font-family: sans-serif;
    user-select: none; /* Standard */
    overflow: auto;
    text-align: center;
    position: relative;
    z-index: -500;
}


