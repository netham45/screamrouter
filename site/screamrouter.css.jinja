DIV#dialog {
    padding-top: 20px;
    padding-bottom: 20px;
    position: absolute;
    min-width: 400px;
    max-width: 95%;
    height: auto;
    top: 12.5%;
    margin-left: -16.5%;
    background-image: linear-gradient(rgba(150,230,221,.7),rgba(120,180,220,.7));
    border-radius: 8px;
    display: none;
    backdrop-filter: blur(5px);
    font-family: "Courier";
    font-weight: 1000;
    text-shadow: 0 0 20px #fff, 0 0 30px #AA7777, 0 0 40px #AA7777;
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    left: 0;
    right: 0;
}

DIV.title {
    font-family: "Courier";
    font-weight: 1000;
    font-size: 48pt;
    text-shadow: 0 0 20px #224422, 0 0 30px #224422, 0 0 40px #224422, 0 0 50px #224422;
    color: rgb(150,230,221);
    text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
    width: 100%;
    overflow: hidden;
    border-collapse: collapse;
    background-color: rgba(00,120,00,.2);
    border-radius: 5px;
    background-size: 60vmin 60vmin;
    display: inline-block;
    border-collapse: collapse;
}

DIV.section {
    pointer-events: auto;   
    overflow: hidden;
    border-collapse: collapse;
    background-color: rgba(00,120,00,.2);
    border-radius: 5px;
    background-size: 60vmin 60vmin;
    width: 33%;
    border-collapse: collapse;
    top: 0px;
    position: relative;
    display: inline-block;
}

DIV.section-name {
    font-family: "Courier";
    font-weight: 1000;
    font-size: 24pt;
    color: rgb(150,230,221);

    text-shadow: 0 0 10px #000, 0 0 15px #99F;
    text-align: center;
    background-image: linear-gradient( rgba(80,160,120,.4), rgba(20,40,30,.1));
}

DIV.section INPUT[TYPE="range"] {
    width: 96%;
    display: inline-block;
    margin-left: 2%;
}

DIV.section-buttons BUTTON {
    font-size: 12pt;
}

DIV.section-buttons {
    text-align: center;
}


DIV#reload {
    height: 33%;
    pointer-events: none;
}

DIV.main-select {
    margin-top: 5px;
    padding-top: 8px;
    font-family: sans-serif;
    overflow: auto;
    text-align: center;
    width: 100%;
    background: none;
    background-image: linear-gradient( rgba(80,160,120,.9), rgba(72,144,108,.7));
    font-size: 1.5vw;
    color: #FFFFFF;
    border: none;
    outline: none;
    padding-bottom: 15px;
    border-radius: 5px;
}

DIV.main-select > SPAN.option:nth-child(even) {
    background-image: linear-gradient(rgba(0,0,0,.2), rgba(0,0,0,.2));
}

DIV#select-sinks > SPAN.option:nth-child(odd) {
    background-image: linear-gradient(rgba(0,0,0,.05), rgba(0,0,0,.05));
}

SPAN.option{
    padding-left: 2%;
    width: 95%;
    display: inline-block;
    text-align: left;

    border: 0px 0px 2px 0px;
    padding-top: 8px;
    padding-bottom: 5px;
    border-radius: 10px;
    color: #000;
    margin-top: .5px;
    margin-bottom: .5px;
}

select[multiple] option:checked {
  background-image: linear-gradient(rgba(140,190,140,.75),rgba(0,140,0,.75),rgba(140,190,140,.75));
}

select[multiple]:focus option:checked {
  background-image: linear-gradient(rgba(220,255,220,.75),rgba(0,192,0,.75),rgba(255,255,255,.75));
}

option:active, option:focus, option:hover, option:checked:after {
    outline: none;
}


HTML, BODY {
    height: 100%;
    background-color: #000100;
    font-family: "Arial";
    user-select: none; /* Standard */
    overflow: auto;
    text-align: center;
    position: relative;
    z-index: -500;
}


